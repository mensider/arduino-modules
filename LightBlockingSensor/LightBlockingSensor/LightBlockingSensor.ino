/*Demonstrates the working of Light Blocking SensorBy Sooraj K*/int led=13; //initializing led pin in digital pin 13int LBsensor=8;// initializing LB sensor in digital pin 8void setup() {  pinMode(led, OUTPUT);// defining led as OUTPUT  Serial.begin(9600);//serial communication at 9600 bps}void loop() {  if (digitalRead(LBsensor)== HIGH) {    digitalWrite(led,HIGH); //turn on the led     Serial.println("HIGH");//print out "HIGH"  }  if (digitalRead(LBsensor)== LOW) {    digitalWrite(led,LOW);//turn of the led  }}